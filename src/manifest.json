{
    "name": "Paws",
    "version": "1.0",
    "description": "Paws Extension",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "assets/images/blackjack.jpg",
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "webRequest"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}